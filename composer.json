{
  "name": "orkestra/transactor-bundle",
  "description": "Integrates the Orkestra Transactor with Symfony 2",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": ">=5.3.3",
    "orkestra/transactor": "~1.1"
  },
  "require-dev": {
    "symfony/framework-bundle": "~3.0",
    "symfony/dependency-injection": "~3.0",
    "symfony/yaml": "~3.0",
    "symfony/form": "~3.0",
    "doctrine/orm": "~2.5"
  },
  "autoload": {
    "psr-0": { "Orkestra\\Bundle\\TransactorBundle": "" }
  },
  "target-dir": "Orkestra/Bundle/TransactorBundle",
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    }
  }
}
